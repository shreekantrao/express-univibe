<!--page content -->
<div class="right_col" id="pagecode_<%=page_Code%>" role="main">
  <div class="">
    <div class="page-title">
      <div class="title_left">
        <h3>Add new College</h3>
      </div>

      <div class="title_right">
        <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
          <div class="input-group">
            <input type="text" class="form-control" placeholder="Search for...">
            <span class="input-group-btn">
              <button class="btn btn-default" type="button">Go!</button>
            </span>
          </div>
        </div>
      </div>
    </div>
    <div class="clearfix"></div>

    <div class="row">

      <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
          <div class="x_title">
            <h2>College Wizards
              <small>Sessions</small>
            </h2>
            <ul class="nav navbar-right panel_toolbox">
              <!-- <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li> -->
              <li><a href="#"><i class="fa fa-wrench"></i></a></li>
              <li><a href="/colleges/list"><i class="fa fa-close"></i></a></li>
            </ul>
            <div class="clearfix"></div>
          </div>
          <div class="x_content">

            <!-- Tabs -->
            <div id="wizard_college_add" class="form_wizard wizard_verticle">
              <ul class="list-unstyled wizard_steps anchor">
                <li>
                  <a href="#step-11" class="selected" isdone="1" rel="1">
                    <span class="step_no">1</span>
                  </a>
                </li>
                <li>
                  <a href="#step-22" class="disabled" isdone="0" rel="2">
                    <span class="step_no">2</span>
                  </a>
                </li>
                <li>
                  <a href="#step-33" class="disabled" isdone="0" rel="3">
                    <span class="step_no">3</span>
                  </a>
                </li>
                <li>
                  <a href="#step-44" class="disabled" isdone="0" rel="4">
                    <span class="step_no">4</span>
                  </a>
                </li>
                <li>
                  <a href="#step-55" class="disabled" isdone="0" rel="5">
                    <span class="step_no">5</span>
                  </a>
                </li>
              </ul>


              <form class="form-horizontal form-label-left addCollegeForm" method="post" action="/colleges/addnew" novalidate>
                <div class="stepContainer" style="height: 528px;">
                  <div id="step-11" class="content" style="display: block;">
                    <h2 class="StepTitle">Step 1</h2>


                    <span class="section">Basic details</span>

                    <div class="form-group field">
                      <label class="control-label col-md-3 col-sm-3" for="name">College Name
                        <span class="required">*</span>
                      </label>
                      <div class="col-md-5 col-sm-5">
                        <input type="text" id="name" name="name" required="required" class="form-control col-md-7 col-xs-12" placeholder="Univibe college" data-validate-length-range="3,15">
                      </div>
                    </div>

                    <div class="form-group field">
                      <label class="control-label col-md-3 col-sm-3" for="slug">Default URL</label>
                      <div class="col-md-3 col-sm-3">
                        <input type="text" id="slug" name="slug" class="form-control col-md-7 col-xs-12" placeholder="univibe-college" required="required" readonly="readonly" >
                      </div>
                      <label class="control-label col-md-2 col-sm-2" style="text-align: left;">.univibe.com</label>
                    </div>

                    <div class="form-group field">
                      <label class="control-label col-md-3 col-sm-3" for="domain">Domain</label>
                      <div class="col-md-5 col-sm-5">
                        <input type="url" id="domain" name="domain" class="form-control col-md-7 col-xs-12" placeholder="univibealumni.com" data-validation="url">
                      </div>
                    </div>

                    <div class="form-group field">
                      <label for="middle-name" class="control-label col-md-3 col-sm-3" for="long_name">College Long Name
                        <span class="required">*</span>
                      </label>
                      <div class="col-md-5 col-sm-5">
                        <input id="long_name" name="long_name" required="required" class="form-control col-md-7 col-xs-12" placeholder="Univibe Alumni Assosiation"
                          type="text" data-validate-length-range="5,25" data-validate-words="2">
                      </div>
                    </div>

                    <div class="form-group field">
                      <label for="middle-name" class="control-label col-md-3 col-sm-3" for="short_name">College Short Name
                        <span class="required">*</span>
                      </label>
                      <div class="col-md-5 col-sm-5">
                        <input id="short_name" name="short_name" required="required" class="form-control col-md-7 col-xs-12" placeholder="Univibe Alumni"
                          type="text" data-validate-length-range="5,15">
                      </div>
                    </div>

                    <div class="form-group field">
                      <label for="middle-name" class="control-label col-md-3 col-sm-3" for="establishment_year">Establishment Year
                        <span class="required">*</span>
                      </label>
                      <div class="col-md-5 col-sm-5">
                        <input id="establishment_year" required="required" class="form-control col-md-7 col-xs-12" type="number" data-validate-minmax="1900,2020">
                      </div>
                    </div>

                    <div class="form-group field">
                      <label class="control-label col-md-3 col-sm-3">Created date</label>
                      <div class="col-md-5 col-sm-5">
                        <input readonly="readonly" class="form-control col-md-7 col-xs-12" type="text">
                      </div>
                    </div>

                    <div class="form-group field">
                      <label class="control-label col-md-3 col-sm-3">Last Edited</label>
                      <div class="col-md-5 col-sm-5">
                        <input readonly="readonly" class="form-control col-md-7 col-xs-12" type="text">
                      </div>
                    </div>

                  </div>
                </div>
                <div class="actionBar">
                  <button type="submit" class="btn btn-primary">Cancel</button>
                  <button id="send" type="submit" class="btn btn-success">Submit</button>
                </div>
              </form>
            </div>
            <!-- End SmartWizard Content -->
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- page content -->