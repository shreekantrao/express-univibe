<!-- page content -->
<div class="right_col" id="pagecode_<%=page_Code%>" role="main">
  <div class="">
    <div class="page-title">
      <div class="title_left">
        <h3>Courses Master
          <small> </small>
        </h3>
      </div>

      <div class="title_right">
        <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
          <div class="input-group">
            <input type="text" class="form-control" placeholder="Search for...">
            <span class="input-group-btn">
              <button class="btn btn-default" type="button">Go!</button>
            </span>
          </div>
        </div>
      </div>
    </div>

    <div class="clearfix"></div>

    <div class="row">
      <div class="col-md-12">
        <div class="x_panel">
          <div class="x_title">
            <h2>Showing -
              <span id="search_total">0</span> /
              <span id="network_total">0</span>
              <!-- <small> master </small> -->
            </h2>
            <ul class="nav navbar-right panel_toolbox">
              <li>
                <a data-popup-open="add">
                  <i class="fa fa-plus"></i>
                </a>
              </li>
              <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">
                  <i class="fa fa-wrench"></i>
                </a>
                <ul class="dropdown-menu" role="menu">
                  <li>
                    <a href="#">Settings 1</a>
                  </li>
                  <li>
                    <a href="#">Settings 2</a>
                  </li>
                </ul>
              </li>
              <li>
                <a href="/dashboard">
                  <i class="fa fa-close"></i>
                </a>
              </li>
            </ul>
            <div class="clearfix"></div>
          </div>
          <div class="x_content">

            <div class="row" id="data-container">

              <div class="col-md-55">
                <div class="thumbnail">
                  <!-- <div class="image view view-first"> -->
                  <a data-popup-add="new">
                    <img style="width: 100%; display: block;" src="/assets/build/images/addItem.png" alt="image" />
                  </a>
                </div>
              </div>

            </div>
            <div id="msgbar" class="pagination-msgbar"></div>

            <!-- modals -->
            <!-- Large modal -->
            <!-- <button type="button" class="btn btn-primary" data-toggle="modal" data-target=".bs-example-modal-lg">Large modal</button> -->
            <div class="modal fade course-modal-form" tabindex="-1" role="dialog" aria-hidden="true">
              <div class="modal-dialog modal-lg">
                <div class="modal-content">

                  <!-- User form -->
                  <form class="form-horizontal form-label-left" id="courseeditform">

                    <div class="modal-header">
                      <span class="popup-close" data-dismiss="modal" href="#">
                        <i class="fa fa-times" aria-hidden="true"></i>
                      </span>
                      <h4 class="modal-title" id="myModalLabel">Add new course.</h4>
                    </div>
                    <div class="modal-body">
                      <div class="row">


                        <div class="col-md-3 col-sm-3 col-xs-12">
                          <!-- Profile details -->
                          <div class="profile_img">
                            <div id="crop-avatar">
                              <!-- Current avatar -->
                              <img onerror="this.src='/assets/build/images/course.jpg'" class="img-responsive avatar-view" src="/assets/build/images/course.jpg"
                                alt="Avatar" title="Change the avatar">
                            </div>
                          </div>
                          <!-- <div style="padding-top: 15px;">
                          <label class="control-label col-md-8 col-sm-8 col-xs-12">Publish</label>
                          <div class="col-md-4 col-sm-4 col-xs-12">
                            <input id="course_status" name="course_status" type="checkbox" checked class="js-switch">
                          </div>
                        </div> -->
                        </div>
                        <div class="col-md-9 col-sm-9 col-xs-12">



                          <!-- Profile line -->
                          <div class="form-group field">
                            <!-- <label class="control-label col-md-2 col-sm-2 col-xs-12">Profile line</label> -->
                            <div class="col-md-7 col-sm-7 col-xs-12">
                              <input id="name" name="name" value="" type="text" class="form-control" placeholder="Course name" required="required">
                              <input id="image" name="image" value="" type="hidden">
                              <input id="slug" name="slug" value="" type="hidden" required="required">
                            </div>
                          </div>

                          <!-- Summary -->
                          <div class="form-group field">
                            <!-- <label class="control-label col-md-2 col-sm-2 col-xs-12">Summary</label> -->
                            <div class="col-md-7 col-sm-7 col-xs-12">
                              <textarea id="description" value="" class="form-control" rows="3" placeholder="Description"></textarea>
                            </div>
                          </div>

                          <!-- <div class="ln_solid"></div> -->

                          <div class="form-group field">
                            <label class="col-md-2 col-sm-2 col-xs-12">Publish</label>
                            <div class="col-md-5 col-sm-5 col-xs-12">
                              <input id="course_status" name="course_status" type="checkbox" checked class="js-switch">

                              <!-- <button type="button" class="btn btn-primary">Cancel</button> -->
                              <!-- <button type="reset" class="btn btn-primary">Reset</button>
                              <button id="course-submit" type="button" class="btn btn-success">Submit</button> -->
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="modal-footer">
                      <!-- <button type="button" class="btn btn-default" data-dismiss="modal">Close</button> -->
                      <button type="reset" class="btn btn-default">Reset</button>
                      <button type="submit" class="btn btn-primary" id="course-submit">Save
                        <i></i>
                      </button>
                    </div>
                  </form>
                </div>
              </div>
            </div>

            <!-- /modals -->
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- /page content -->