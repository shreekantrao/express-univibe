                    <!-- name -->
                    <div class="form-group field">
                      <label class="control-label col-md-3 col-sm-3" for="name">College Name
                        <span class="required">*</span>
                      </label>
                      <div class="col-md-5 col-sm-5">
                        <input type="text" id="name" name="name" value="<%-data.name%>" required="required" class="form-control col-md-7 col-xs-12" placeholder="Univibe college" data-validate-length-range="3,15" readonly="readonly">
                      </div>
                    </div>
                    <!-- slug -->
                    <div class="form-group field">
                      <label class="control-label col-md-3 col-sm-3" for="slug">Default URL</label>
                      <div class="col-md-3 col-sm-3">
                        <input type="text" id="slug" name="slug" value="<%-data.slug%>" class="form-control col-md-7 col-xs-12" placeholder="univibe-college" required="required" readonly="readonly" >
                      </div>
                      <label class="control-label col-md-2 col-sm-2" style="text-align: left;">.univibe.com</label>
                    </div>
                    <!-- domain -->
                    <div class="form-group field">
                      <label class="control-label col-md-3 col-sm-3" for="domain">Domain</label>
                      <div class="col-md-5 col-sm-5">
                        <input type="url" id="domain" name="domain" value="<%-data.domain%>" class="form-control col-md-7 col-xs-12" placeholder="univibealumni.com" data-validation="url">
                      </div>
                    </div>
                    <!-- long_name -->
                    <div class="form-group field">
                      <label for="middle-name" class="control-label col-md-3 col-sm-3" for="long_name">College Long Name
                        <span class="required">*</span>
                      </label>
                      <div class="col-md-5 col-sm-5">
                        <input id="long_name" name="long_name" value="<%-data.long_name%>" required="required" class="form-control col-md-7 col-xs-12" placeholder="Univibe Alumni Assosiation"
                          type="text" data-validate-length-range="5,25" data-validate-words="2">
                      </div>
                    </div>
                    <!-- short_name -->
                    <div class="form-group field">
                      <label for="middle-name" class="control-label col-md-3 col-sm-3" for="short_name">College Short Name
                        <span class="required">*</span>
                      </label>
                      <div class="col-md-5 col-sm-5">
                        <input id="short_name" name="short_name" value="<%-data.short_name%>" required="required" class="form-control col-md-7 col-xs-12" placeholder="Univibe Alumni"
