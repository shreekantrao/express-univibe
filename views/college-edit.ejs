                    </div>
                    <!-- registered_date -->
                    <div class="form-group field">
                      <label class="control-label col-md-3 col-sm-3">Created date</label>
                      <div class="col-md-5 col-sm-5">
                        <input readonly="readonly" value="<%-new Date(data.registered_date).toLocaleDateString()%>" class="form-control col-md-7 col-xs-12" type="text">
